foreach %aeid in @arr
    start ->
        receive
            type = "cli", advertisable_eid = %aeid -> yield $match, yield timestamp to #timestamps
            type = "ttt" -> yield cookie,cookie_timestamp_filter_end,cookie_timestamp_filter_start to &ttt
            * -> repeat

